  <script>
    $(function() {
      var ctx = $("#myChart").get(0).getContext("2d");
      var data = {
        labels: ["推荐指数", "学费", "学生人数", "学校历史", "校园面积"],
        datasets: [{
          fillColor: "#F0AD4E",
          strokeColor: "#F0AD4E",
          pointColor: "#F0AD4E",
          pointStrokeColor: "#fff",
          data: [60, 60, 60, 60, 60]
        }, {
          fillColor: "#5CB85C",
          strokeColor: "#5CB85C",
          pointColor: "#5CB85C",
          pointStrokeColor: "#fff",
          data: [70, 70, 40, 70, 55]
        }]
      };
      var aa = new Chart(ctx).Radar(data, {
        animation: true,
        scaleShowLabels: true,
        scaleFontColor: "#767C8D",
        scaleOverlay: true,
        scaleLabel: "<%=value%>%",
        scaleShowLabelBackdrop: false,
        pointLabelFontSize: 12,
        angleLineWidth: 3,
        datasetStrokeWidth: 3
      });
    });
